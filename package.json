{
  "name": "wikiatlas",
  "version": "0.0.2",
  "description": "Wikimaps Atlas (2020)",
  "main": "Wikimaps Atlas (2020) is a piece of code to output nice web-friendly maps, administrative and physical. It installs relevant GIS resources, software dependencies, then can be ran on demand for a target area. See README.md for the API.",
  "dependencies": {
    "d3": "~3.5.5",
    "forever": "^3.0.2",
    "http-server": "^0.12.3",
    "jsdom": "^16.4.0",
    "minimist": "^1.2.5",
    "shapefile": "^0.6.6",
    "topojson": "^3.0.2",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<...>/<...>.git"
  },
  "bugs": {
    "url": "https://github.com/<...>/<...>/issues"
  },
  "author": "Hugo lopez <hugo.lpz@gmail.com>, Arun Ganesh <arun.planemad@gmail.com>",
  "keywords": [
    "map",
    "d3js"
  ],
  "scripts": {
    "install-make": "make -f install.makefile",
    "data": "make -f data.makefile core",
    "dataplus": "make -f data.makefile precise",
    "serve": "node ./node_modules/.bin/forever ./node_modules/.bin/http-server"
  },
  "license": "CC-BY-SA-4.0"
}
